{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.Authorization": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "#{Key}#",
    "Issuer": "#{Issuer}#",
    "Audience": "#{Audience}#",
    "ExpiresInMinutes": "#{ExpiresInMinutes}#"
  }

}
